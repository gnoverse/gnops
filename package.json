{
  "name": "gnome-web",
  "version": "1.0.0",
  "description": "gnome web interface",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:hugo": "hugo --watch --gc -e default --printUnusedTemplates --printI18nWarnings",
    "dev:search": "npx pagefind --site dist",
    "dev:vite": "vite --host",
    "dev": "rm -rf dist/* && run-p dev:hugo dev:search dev:vite",
    "preview:hugo": "hugo -e default",
    "preview": "rm -rf build/* && rm -rf dist/* && run-s preview:hugo build:vite",
    "build:hugo": "hugo -e production",
    "build:vite": "tsc && vite build",
    "build": "rm -rf build/* && rm -rf dist/* && run-s build:hugo build:vite"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "dependencies": {
    "@barba/core": "^2.10.3",
    "@fontsource-variable/inter": "^5.0.19",
    "@fontsource/inter": "^5.0.19",
    "@types/node": "^22.10.1",
    "gsap": "^3.12.5"
  }
}
